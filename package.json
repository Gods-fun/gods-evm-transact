{
  "name": "@ai16z/agent",
  "version": "0.1.1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "format": "biome format --write .",
    "lint": "biome lint .",
    "check": "biome check --apply .",
    "build": "npx turbo run build",
    "start": "NODE_ENV=production tsx src/index.ts",
    "start:service:all": "pm2 start pnpm --name=\"all\" --restart-delay=3000 --max-restarts=10 -- run start:all",
    "stop:service:all": "pm2 stop all",
    "start:client": "pnpm --dir client dev",
    "clean": "rm -rf node_modules **/node_modules pnpm-lock.yaml **/pnpm-lock.yaml",
    "install:all": "pnpm install && pnpm -r install"
  },
  "dependencies": {
    "@elizaos/adapter-postgres": "0.1.7",
    "@elizaos/adapter-supabase": "workspace:*",
    "@elizaos/adapter-sqlite": "0.1.7",
    "@elizaos/client-auto": "0.1.7",
    "@elizaos/client-direct": "0.1.7",
    "@elizaos/client-discord": "0.1.7",
    "@elizaos/client-telegram": "0.1.7",
    "@elizaos/client-twitter": "workspace:*",
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-bootstrap": "0.1.7",
    "@elizaos/plugin-evm": "workspace:*",
    "@elizaos/plugin-image-generation": "workspace:*",
    "@elizaos/plugin-nft-generation": "workspace:*",
    "@elizaos/plugin-node": "0.1.7",
    "@elizaos/plugin-solana": "workspace:*",
    "@elizaos/plugin-starknet": "0.1.7",
    "@tavily/core": "0.0.2",
    "amqplib": "0.10.5",
    "better-sqlite3": "11.5.0",
    "fs": "0.0.1-security",
    "net": "1.0.2",
    "path": "0.12.7",
    "readline": "1.3.0",
    "url": "0.11.4",
    "ws": "8.18.0",
    "yargs": "17.7.2"
  },
  "engines": {
    "node": "23.3.0"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.0",
      "viem": "2.21.58"
    }
  },
  "packageManager": "pnpm@9.15.0",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.3",
    "@types/jest": "^29.5.11",
    "concurrently": "9.1.0",
    "@types/node": "^22.8.4",
    "cross-env": "7.0.3",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "lerna": "8.1.5",
    "only-allow": "1.2.1",
    "turbo": "2.3.3",
    "typedoc": "0.26.11",
    "typescript": "5.6.3",
    "viem": "2.21.58",
    "vite": "5.4.12",
    "vitest": "2.1.5"
  },
  "workspaces": [
    "packages/*"
  ]
}
